<header id="top-header">
    <span class="cse-motors">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">
        <% if (locals.loggedin) { %>
            <a href="/account/" title="Click to go to account management">Welcome, <%- locals.accountData.account_firstname %></a> |
            <a href="/account/logout/" title="Click to log out">Log Out</a>
        <% } else { %>
            <a title="Click to log in" href="/account/login">My Account</a>
        <% } %>
    </div>
</header>